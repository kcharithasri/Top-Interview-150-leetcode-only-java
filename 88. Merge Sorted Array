class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int i=m+n;
        int j=0;
        while(m<i){
            nums1[m]=nums2[j];
            m++;
            j++;
        }
        Arrays.sort(nums1);
        for(int k=0;k<i;k++)
        System.out.print(nums1[k]+" ");
    }
}
